                Zepto(function($){
                    var ENDORSEMENTS_TEMPLATE = '<li>\
                            [[#isPositive]]\
                                [[#isLove]]\
                                    <img src="endorse-with-transparent-star@2x.png" width="16" height="16" />\
                                [[/isLove]]\
                                [[^isLove]]\
                                    <img src="thumbs-up@2x.png" width="16" height="17" />\
                                [[/isLove]]\
                            [[/isPositive]]\
                            [[#isNegative]]\
                                <img src="thumbs-down@2x.png" width="16" height="17" />\
                            [[/isNegative]]\
                            <div class="review">\
                                [[#isLove]]\
                                <div>from [[&author]]: "[[&review]]"[[#skill]] (Skill: [[skill]])[[/skill]]</div>\
                                [[/isLove]]\
                                [[^isLove]]\
                                <div>[[&review]]</div>\
                                [[/isLove]]\
                            </div>\
                        </li>';
                    var ORIGINAL_DATA = {{{originalData}}};
                    var LIMIT = 20;
                    var ORIGINAL_DATA_MAX_COUNT = 10;
                    var lastOffset = ORIGINAL_DATA.reviews.length;

                    var renderReviews = function(reviews) {
                        for (var i in reviews) {
                            var review = reviews[i];
                            
                            if (Number(review.rating) > 0) {
                                review.isPositive = true;
                            } else if (Number(review.rating) < 0) {
                                review.isNegative = true;
                            }
                            
                            if (Number(review.is_love)) {
                                review.isLove = true;
                            }

                            $('#reviews-load-more').before(Mustache.render(ENDORSEMENTS_TEMPLATE, review));
                        }
                    }
                    
                    var sendRecomputeWebViewHeightSignal = function() {
                        document.location = 'recompute-webview-height://';
                    };
                    
                    var setLoadMoreText = function(text) {
                        $('#reviews-load-more a').text(text);
                    }

                    renderReviews(ORIGINAL_DATA.reviews);
                    if (ORIGINAL_DATA.reviews.length < ORIGINAL_DATA_MAX_COUNT) {
                        $('#reviews-load-more').hide();
                    }
                    sendRecomputeWebViewHeightSignal();

                    $('#reviews-load-more').on('click', 'a', function(e) {
                        $.ajax({
                            url: ORIGINAL_DATA.server_api_url,
                            data: {
                                action: 'getLoveForUserID',
                                user_id: ORIGINAL_DATA.user_id,
                                limit: LIMIT,
                                offset: lastOffset
                            },
                            dataType: 'json',
                            context: $(e.target),
                            success: function(json) {
                                if (json && !json.error && json.payload != null && json.payload.rows != null) {

                                    setLoadMoreText('Load more...');

                                    if (json.payload.rows.length) {
                                        renderReviews(json.payload.rows);

                                        lastOffset += json.payload.rows.length;
                                        
                                        $(e.target).show();
                                        
                                        sendRecomputeWebViewHeightSignal();
                                    } else {
                                        $(e.target).hide();
                                        sendRecomputeWebViewHeightSignal();
                                    }
                                } else {
                                    setLoadMoreText('Error loading more');
                                }
                            },
                            error: function(xhr, errorType, error) {
                                setLoadMoreText('Error loading more');
                            }
                        });
                        
                        return false;
                    });
                })
            </script>
        {{/hasAnyReview}}
    </div>
</body>
</html>

